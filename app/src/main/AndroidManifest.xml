<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uagrm.rodrigoab.agendakotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono_logo"
        android:logo="@mipmap/icono_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".helpers.AlarmNotificationReceiver" />

        <activity
            android:name=".activities.Formulario"
            android:parentActivityName=".activities.MainActivity"></activity>
        <activity
            android:name=".activities.VistaAnual"
            android:parentActivityName=".activities.MainActivity"></activity>
    </application>

</manifest>